{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "pnpm quasar build",
    "beforeDevCommand": "pnpm quasar dev",
    "frontendDist": "../dist/spa",
    "devUrl": "http://localhost:9000"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": ["en-US", "zh-CN"]
      },
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "OllamaOne",
  "version": "0.1.0",
  "identifier": "com.jthinking.ollamaone",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 700,
        "resizable": true,
        "title": "OllamaOne",
        "width": 1024,
        "center": true,
        "decorations": true,
        "visible": false,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true
      },
      {
        "width": 200,
        "height": 200,
        "center": true,
        "decorations": false,
        "url": "splashscreen.html",
        "label": "splashscreen",
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
